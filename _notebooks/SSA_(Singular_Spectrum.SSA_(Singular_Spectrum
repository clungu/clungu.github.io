       "         6.96965055,   6.87384928,   6.65348868,   6.59733372,\n",
       "         6.47399648,   6.32487688,   6.20118721,   5.96992107,\n",
       "         5.836588  ,   5.73879453,   5.71891451,   5.63213598,\n",
       "         5.4930785 ,   5.3414355 ,   5.30227011,   4.99551775,\n",
